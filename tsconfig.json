{
  "extends": "./node_modules/gts/tsconfig-google.json",
  "compilerOptions": {
    "rootDir": ".",
    "outDir": "build",
    "module": "ESNext",
    "lib": [
      "ESNext",
      "DOM",
      "DOM.Iterable"
    ],
    "moduleResolution": "Bundler",
    "jsx": "preserve",
    "jsxImportSource": "vue",
  },
  "include": [
    "src/**/*.vue",
    "src/**/*.ts",
  ]
}
